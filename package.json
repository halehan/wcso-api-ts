{
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "name": "wcso-api-ts",
  "version": "1.0.0",
  "description": "WCSO api using TypeScript",
  "engines": {
    "node": "8.5.0",
    "npm": "5.4.2"
  },
  "main": "app.js",
  "scripts": {
    "compile": "tsc --declaration",
    "lint": "tslint --project tsconfig.json",
    "test": "nodemon --watch . --ext ts --exec \"mocha -r ts-node/register test/**/*.spec.ts\"",
    "test-dev": "tsc nodemon --watch . --ext ts --exec \"mocha -r ts-node/register test/**/*.spec.ts\"",
    "start": "node --inspect=5858 -r ts-node/register ./src/app.ts",
    "start:watch": "nodemon",
    "build": "tsc",
    "ts": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halehan/wcso-api-ts.git"
  },
  "author": "Thomas D. Williams",
  "license": "ISC",
  "dependencies": {
    "@composite/equals-ignore-case": "^1.0.0",
    "@types/express-serve-static-core": "^4.17.3",
    "@types/moment": "^2.13.0",
    "@types/superagent": "^4.1.7",
    "async": "^2.6.3",
    "bcrypt": "^1.0.3",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^4.0.0",
    "express": "^4.17.1",
    "facebook-chat-api": "^1.8.0",
    "googlemaps": "^1.12.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.7",
    "morgan": "^1.10.0",
    "node-restful": "^0.2.5",
    "oauth": "^0.9.15",
    "passport": "^0.4.1",
    "twilio": "^3.42.0",
    "typings": "^2.1.1",
    "https-proxy-agent": ">=2.2.0"
  },
  "bugs": {
    "url": "https://github.com/halehan/wcso-api-ts/issues"
  },
  "homepage": "https://github.com/halehan/wcso-api-ts#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/superagent": "^4.1.7",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "nodemon": "^1.19.4",
    "supertest": "^4.0.2",
    "ts-essentials": "^6.0.4",
    "ts-node": "^8.8.1",
    "typescript": "^3.7.5"
  }
}
